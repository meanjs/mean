{
	"node": true,
	"browser": true,
	"esnext": true,
	"bitwise": false,
	"camelcase": false,
	"curly": false,
	"eqeqeq": true,
	"immed": true,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"quotmark": "single",
	"regexp": true,
	"undef": true,
