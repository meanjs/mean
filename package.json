    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "semver": "~5.4.1",
    "should": "~11.2.1",
    "supertest": "~3.0.0"
  }
}
